/*
---
name: lightSpeedOut
category: Animations
---
```example.html
<svg class="icon animated lightSpeedOut infinite" role="presentation">
  <use xlink:href="../dist/svg/symbol/svg/sprite.symbol.svg#twitter"></use>
</svg>
```
*/
@keyframes lightSpeedOut {
  from {
    opacity: 1;
  }

  to {
    transform: translate3d(100%, 0, 0) skewX(30deg);
    opacity: 0;
  }
}

.lightSpeedOut {
  animation-name: lightSpeedOut;
  animation-timing-function: ease-in;
}
